@model Domain.Models.ReporteModel

<!-- Modal -->
<div class="modal fade" id="modal-panel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Datos de Entidad</h4>
        </div>
        <div class="modal-body">
            <form class="form-horizontal">
            <fieldset>
                <div class="form-group">
                    <label class="control-label">Nro. Trámite:</label>
                    <em>@Model.NroTramite</em>
                </div>
                <div class="form-group">
                    <label class="control-label">Timbrado:</label>
                    <em>@Model.CodigoFormulario</em>
                </div>
                <div class="form-group">
                    <label class="control-label">Nro. Presentación:</label>
                    <em>@Model.NroTramite</em>
                </div>
                <div class="form-group">
                    <label class="control-label">Nro Correlativo:</label>
                    <em>@Model.NroCorrelativo</em>
                </div>
                <div class="form-group">
                    <label class="control-label">Razón Social:</label>
                    <em>@Model.Denominacion</em>
                </div>
                <div class="form-group">
                    <label class="control-label">Tipo Societario:</label>
                    <em>@Model.TipoSocietario</em>
                </div>
            </fieldset>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            <button type="submit" class="btn btn-default">Imprimir</button>
        </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script type="text/javascript">
    $(document).ready(function () {
        $('#modal-panel').modal('show');
    });
</script>
